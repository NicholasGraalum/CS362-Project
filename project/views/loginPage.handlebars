<main class = "login">
    <div class="login-page-header">
        <h1>Welcome Back</h1>
    </div>
    <div class="login-details">    
        <p>Email: </p> 
        <input type="text" class="login-box" id="submit-email"></input>
        <p>Password: </p> 
        <input type="text" class="login-box" id="submit-password"></input>
    </div>
    <div class="login-button">
        <button class="edit-button" id="submit-login">Login</button>
    </div>
    <div class="login-button">
        <p>Dont have an account?<button class="edit-button" id="create-profile">Signup</button></p>
    </div>
</main>